{
  "_version": "1.12.0",
  "sap.app": {
    "id": "recruitment.task",
    "type": "application",
    "title": "Zadanie rekrutacyjne TOYA S.A.",
    "description": "Zadanie rekrutacyjne TOYA S.A.",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "northwind": {
        "uri": "/northwind/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "recruitment.task.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "preload": "none",
    "models": {
      "": {
        "dataSource": "northwind",
        "settings": {
          "useBatch": false
        }
      }
    },
    "dependencies": {
      "minUI5Version": "1.71.70",
      "libs": {
        "sap.m": {}
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "recruitment.task.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "home",
          "target": "home"
        },
        {
          "pattern": "customers",
          "name": "customers",
          "target": "customers"
        }
      ],
      "targets": {
        "home": {
          "viewName": "Home",
          "viewLevel": 1,
          "viewId": "home"
        },
        "customers": {
          "viewName": "Customers",
          "viewLevel": 2,
          "viewId": "customers"
        }
      }
    }
  }
}
